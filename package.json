        "patch": "npm version patch && npm run vsce",
        "vsce": "rm quantum-leap-*; vsce package && vsce publish"
    },
    "icon": "icon-nodejs-dark.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/firehawk09/quantum-node-pack"
    },
